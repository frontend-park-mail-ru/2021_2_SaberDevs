{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": [
      "*.pug.js",
      "globals.js",
      "public/components/_template/*.js",
      "public/flux/reducers/template.js",
      "dist/*.js"
    ],
    "rules": {
    },
    "overrides": [
        {
          "files": [
            "*.js"
          ],
          "rules": {
            "linebreak-style": "off",
            "no-multi-spaces": "off",
            "guard-for-in": "off"
          }
        }
    ]
}
