.profile
  .profile__info
    img.profile__photo(src=`${avatarUrl || 'static/img/users/user_icon_loading.svg'}`)
    .profile__description
      .profile__username #{firstName} #{lastName} (#{login})
      .profile__aboutme Обо мне:
      | #{description}
  if (!disableArticles)   
    .profile__articles-title Статьи